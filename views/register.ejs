<% layout('layout') -%>
<form action="/register" method="post">
	<div>
		<label>Email:</label>
		<input type="text" name="email"/>
	</div>
	<div>
		<label>Username:</label>
		<input type="text" name="username"/><br/>
	</div>
	<div>
		<label>Password:</label>
		<input type="password" name="password"/>
	</div>
	<div>
		<input type="submit" value="Submit"/>
	</div>
</form>
